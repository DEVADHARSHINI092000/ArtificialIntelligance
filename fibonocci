n=int(input("HOW MANY TERMS?"))
f1=0
f2=1
count=0
if n<=0:
    print("enter the positive number")
elif n==1:
    print("THE FIBONNACI SEQUENCE IS:",f1)
else:
   print("FIBONACCI SEQUENCE UPTO",n,":")
   while count < n:
       print(f1)
       fth = f1 + f2
       f1 = f2
       f2 = fth
       count += 1
